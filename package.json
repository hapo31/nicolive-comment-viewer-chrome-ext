{
  "name": "nicolive-comment-viewer",
  "version": "1.0.0",
  "main": "index.tsx",
  "author": "happou31 <happo31@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack --config ./webpack.config.js --watch --devtool source-map --mode development --module-bind='svg=react-svg-loader' --module-bind 'ts=ts-loader' --module-bind 'tsx=ts-loader' --resolve-extensions .ts,.tsx,.js,.svg",
    "build": "webpack --config ./webpack.config.js --mode production --module-bind='svg=react-svg-loader' --module-bind 'ts=ts-loader' --module-bind 'tsx=ts-loader' --resolve-extensions .ts,.tsx,.js,.svg",
    "clean": "rm -rf extension & rm -rf debug"
  },
  "dependencies": {
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "styled-components": "^3.2.6",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.65",
    "@types/react": "^16.3.1",
    "@types/react-dom": "^16.0.4",
    "copy-webpack-plugin": "^4.5.1",
    "prettier": "^1.11.1",
    "react-svg-loader": "^2.1.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  }
}
